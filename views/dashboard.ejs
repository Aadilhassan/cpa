<script type="text/javascript">
    function copyToClipboard(element) {
        var $temp = $("<input>");
        $("body").append($temp);
        $temp.val($(element).parents('.code-container').find('.code span').text()).select();
        document.execCommand("copy");
        $temp.remove();
    }

    $(function() {
        $.getJSON("https://cpalead.com/dashboard/reports/campaign_json.php?id=1678354&subid=<%= user.userid %>&ua=user&format=JSONP&callback=?",
            function(data)
            {

                let offers = '';
                $.each(data.offers, function(o, offer)

                {
                    let  mon = Math.floor(offer.amount/2 * 100) / 100
                    offers += '<div id="tab" " class="list-group-item"  >' +
                        '<a href="' + offer.link + ' xmlns="http://www.w3.org/1999/html">'
                        +
                        '<h4>'+offer.title+ '</h4>' +
                        '<div id="coun">country:<country>' +offer.country + '</country></div>'+
                        '<span style="float: right"> $'+ mon+'</span>'+'<div style="float: left"> Type:'   + offer.conversion +'' +
                        '</div>' +
                        '</a>'  +'<div class="code-container">'+
                       ' <span id="c2" class="code" hidden><span style="color:#59c053;">'+ offer.link +'</span></span>'+
                '<button style="margin-left:5px;float:right;" onclick="copyToClipboard(this)" type="button" class="btn btn-outline-dark btn-xs">copie Link</button>'
                +' </div>'  +'</div>'


                ;
                });
                $(".offer-list").html(offers);
                $(".progress").hide();
            });
    });
</script>
