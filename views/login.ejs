 <div class="masthead-content">
  <div class="container full-height">
   <div class="row flex center v-center full-height">
<!--    <div class="col-8 col-sm-4">-->
     <center>

       
       <div class="card bg-transparent shadow-lg p-3 mb-5 bg-body rounded"    style="min-width: 220px; max-width: 400px;  margin-top: 0px ">
      <form action="/login" method="post">
       <fieldset>
        <legend>Log in</legend>
        <img
                id="avatar"
                class="rounded-circle"   style="width:100px; margin-:" 
   20px   ; border-radius:100px          src="static/img/avatar.png"
        />
          <% if(message.length > 0) { %>
            <div class="alert alert-danger"><%= message %></div>
           <% } %>


             
             <div class="input-group mb-3 id="inputGroup-sizing-sm">
  <span class="input-group-text" id="basic-addon1">email</span>
        <input
                class="form-control"
                type="email"
                id="username"
                name="username" 
                aria-describedby="basic-addon1" placeholder="email"
          required
        />
</div>
             <div class="input-group mb-3">
  <span class="input-group-text" id="basic-addon1">password</span><input
                class="form-control"
                type="password"
                id="password"
                name="password"
                aria-describedby="basic-addon1" placeholder="password"
                                                                    required
        />
             </div> 
             <div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault pass" onclick="pass()" >
  <label class="form-check-label" for="flexCheckDefault" style="float:left">      View Password
  </label>
</div>
             <button
                class="btn btn- d-block w-100"
                type="submit"
        >
         LOGIN
        </button>
       </fieldset>
      </form>
<!--      <a href="forgot">forgotten password?</a>-->
